{
  "window.titleBarStyle": "native",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
  "window.menuBarVisibility": "hidden",
  "workbench.colorTheme": "Flare Theme",
  "workbench.activityBar.location": "top",
  "workbench.statusBar.visible": false,
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "symbols",
  "emulator.emulatorPathLinux": "~/Android/Sdk/emulator",
  "workbench.colorCustomizations": {
    "terminal.foreground": "#ebdbb2",
    "terminal.ansiBlack": "#1e2127",
    "terminal.ansiBlue": "#61afef",
    "terminal.ansiCyan": "#56b6c2",
    "terminal.ansiGreen": "#98c379",
    "terminal.ansiMagenta": "#c678dd",
    "terminal.ansiRed": "#e06c75",
    "terminal.ansiWhite": "#abb2bf",
    "terminal.ansiYellow": "#d19a66",
    "terminal.ansiBrightBlack": "#5c6370",
    "terminal.ansiBrightBlue": "#61afef",
    "terminal.ansiBrightCyan": "#56b6c2",
    "terminal.ansiBrightGreen": "#98c379",
    "terminal.ansiBrightMagenta": "#c678dd",
    "terminal.ansiBrightRed": "#e06c75",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#d19a66",
    "terminal.selectionBackground": "#ebdbb2"
  },
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Light",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.enable": true,
  "eslint.format.enable": true,
  "editor.formatOnSave": true,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSet": "default",
  "errorLens.gutterIconSize": "80%",
  "editor.accessibilitySupport": "off",
  "editor.semanticHighlighting.enabled": false,
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.renderLineHighlight": "gutter",
  "editor.fontFamily": "FiraCode Nerd Font Light",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.5,
  "editor.tabSize": 2,
  "editor.autoClosingBrackets": "always",
  "explorer.compactFolders": false,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.autoClosingTags": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.autoClosingTags": true,
  "security.workspace.trust.untrustedFiles": "open",
  "diffEditor.ignoreTrimWhitespace": false,
  "extensions.ignoreRecommendations": true,
  "window.commandCenter": false,
  "explorer.confirmDragAndDrop": false,
  "chat.commandCenter.enabled": false,
  "symbols.hidesExplorerArrows": true,
  "sqldeveloper.sqlHistory.historyLimit": 500,
  "sqldeveloper.telemetry.enabled": false,
  "github.copilot.enable": {
    "*": true
  },
  "window.customTitleBarVisibility": "never"
}